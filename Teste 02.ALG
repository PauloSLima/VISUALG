Algoritmo "semnome"

Var

salario : real
  nome : caractere
  Idade : inteiro

Inicio

      idade <- 32
      salario <- 4560
      nome <- "Maria Silva"

      escreval("A funcionaria ", nome)
      escreval
      escreval( "ganha ",  salario:8:2)
      escreval
      escreval( "e tem ", idade, "anos")


Fimalgoritmo