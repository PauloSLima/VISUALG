Algoritmo "semnome"

Var

   area : real
   b1,B2,h : real     // visual não aceita variavel com difenreça apenas de maisculo e minuscolo
                      // para diferenciar deve ser tratado maisculo e minisculo

Inicio

      b1 <- 6.0
      B2 <- 8.0
      h <- 5.0
      
      area <- (b1+B2)/2.0*h;
      
      escreval (area)
      
      


Fimalgoritmo